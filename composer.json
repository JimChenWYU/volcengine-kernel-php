{
    "name": "jimchen/volcengine-kernel-php",
    "description": "火山引擎 PHP SDK 核心.",
    "license": "MIT",
    "authors": [
        {
            "name": "JimChen",
            "email": "imjimchen@163.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "pimple/pimple": "^1.0 || ^2.0 || ^3.0",
        "monolog/monolog": "^1.0 || ^2.0",
        "symfony/http-foundation": "^3.0 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Volcengine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Volcengine\\Tests\\": "tests"
        }
    }
}
